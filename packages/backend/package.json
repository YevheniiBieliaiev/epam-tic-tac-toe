{
  "name": "@tic-tac-toe/backend",
  "version": "0.0.1",
  "description": "Pet-project 'TIC-TAC-TOE'/backend",
  "private": true,
  "main": "./src/server.ts",
  "engines": {
    "npm": ">=8.0.0 <10.0.0",
    "node": ">=8.10.0"
  },
  "prettier": "@tic-tac-toe/prettier-config",
  "scripts": {
    "start": "nodemon -r ts-node --files ./src/server.ts",
    "build": "tsc && tsc-alias",
    "serve": "node build/server.js",
    "db:generate": "prisma generate",
    "db:migrate:create": "prisma migrate dev --create-only --name",
    "db:migrate": "prisma migrate dev",
    "prisma:studio": "npx prisma studio",
    "prettier:check": "prettier --check \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "prettier:fix": "prettier --write \"*/**/*.{ts,tsx}\" --loglevel=warn",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "author": "Yevhenii Bieliaiev <yevheniibieliaiev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.209.0",
    "@prisma/client": "^5.1.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/pug": "^2.0.6",
    "cookie": "^0.5.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "juice": "^8.1.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^2.0.20",
    "pug": "^3.0.2",
    "sib-api-v3-typescript": "^2.2.2",
    "socket.io": "^4.7.2",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@tic-tac-toe/prettier-config": "file:../prettier-config",
    "@tic-tac-toe/shared": "file:../shared/build",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.8.2",
    "prettier": "^2.7.1",
    "prisma": "^5.1.1",
    "prisma-erd-generator": "^1.2.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0"
  }
}
